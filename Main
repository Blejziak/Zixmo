-- Funkcje przeciƒÖgania
local dragging = false
local dragStart = nil
local startPos = nil

TopBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - dragStart
        MainFrame.Position = UDim2.new(
            startPos.X.Scale,
            startPos.X.Offset + delta.X,
            startPos.Y.Scale,
            startPos.Y.Offset + delta.Y
        )
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)

-- Funkcja weryfikacji i ≈Çadowania skryptu
local function LoadScript()
    -- Tutaj mo≈ºesz za≈Çadowaƒá g≈Ç√≥wny skrypt po weryfikacji
    print("Key verified! Loading script...")
    -- Przyk≈Çad: loadstring(game:HttpGet("URL_DO_SKRYPTU"))()
    -- Mo≈ºesz te≈º pokazaƒá nowe GUI z funkcjami skryptu
end

-- Obs≈Çuga przycisk√≥w
CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

CheckKeyButton.MouseButton1Click:Connect(function()
    local enteredKey = KeyInputBox.Text
    if enteredKey == "" then
        -- Animacja b≈Çƒôdu
        KeyInputBox.Text = ""
        KeyInputBox.PlaceholderText = "Please enter a key!"
        TweenService:Create(KeyInputBox, TweenInfo.new(0.1), {BackgroundColor3 = Color3.fromRGB(200, 50, 50)}):Play()
        wait(0.1)
        TweenService:Create(KeyInputBox, TweenInfo.new(0.1), {BackgroundColor3 = Color3.fromRGB(30, 30, 40)}):Play()
        KeyInputBox.PlaceholderText = "Enter your key here..."
        return
    end
    
    if VerifyKey(enteredKey) then
        -- Klucz poprawny
        CheckKeyButton.Text = "Key Verified!"
        CheckKeyButton.BackgroundColor3 = Color3.fromRGB(50, 200, 100)
        LoadScript()
        wait(1)
        ScreenGui:Destroy() -- Zamknij GUI weryfikacji
    else
        -- Klucz niepoprawny
        CheckKeyButton.Text = "Invalid Key!"
        CheckKeyButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
        KeyInputBox.Text = ""
        wait(1)
        CheckKeyButton.Text = "Check Key"
        CheckKeyButton.BackgroundColor3 = Color3.fromRGB(50, 150, 100)
    end
end)

GetKeyLinkButton.MouseButton1Click:Connect(function()
    -- Tutaj mo≈ºesz dodaƒá link do Platoboost
    local keyLink = "https://example.com/get-key" -- Zamie≈Ñ na prawdziwy link
    pcall(function()
        setclipboard(keyLink)
    end)
    GetKeyLinkButton.Text = "Link Copied!"
    wait(1)
    GetKeyLinkButton.Text = "Get Key Link"
end)

JoinDiscordButton.MouseButton1Click:Connect(function()
    -- Tutaj mo≈ºesz dodaƒá link do Discord
    local discordLink = "https://discord.gg/example" -- Zamie≈Ñ na prawdziwy link
    pcall(function()
        setclipboard(discordLink)
    end)
    JoinDiscordButton.Text = "Link Copied!"
    wait(1)
    JoinDiscordButton.Text = "Join Discord"
end)

-- Efekty hover
local function AddHoverEffect(button, normalColor, hoverColor)
    button.MouseEnter:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = hoverColor}):Play()
    end)
    button.MouseLeave:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = normalColor}):Play()
    end)
end

AddHoverEffect(CloseButton, Color3.fromRGB(200, 50, 50), Color3.fromRGB(255, 70, 70))
AddHoverEffect(CheckKeyButton, Color3.fromRGB(50, 150, 100), Color3.fromRGB(60, 180, 120))
AddHoverEffect(GetKeyLinkButton, Color3.fromRGB(100, 100, 200), Color3.fromRGB(120, 120, 240))
AddHoverEffect(JoinDiscordButton, Color3.fromRGB(88, 101, 242), Color3.fromRGB(105, 115, 255))
AddHoverEffect(SidebarKeyButton, Color3.fromRGB(50, 100, 200), Color3.fromRGB(60, 120, 240))

-- Aktualizacja rozmiaru ContentScrolling
ContentLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
    ContentScrolling.CanvasSize = UDim2.new(0, 0, 0, ContentLayout.AbsoluteContentSize.Y + 20)
end)

print("Verification GUI za≈Çadowane!")
