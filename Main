-- GUI Script z Key System dla Roblox Executor
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local HttpService = game:GetService("HttpService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Lista wa≈ºnych kluczy (mo≈ºesz dodaƒá wiƒôcej lub po≈ÇƒÖczyƒá z API)
local ValidKeys = {
    -- Przyk≈Çadowe klucze - w produkcji powinny byƒá pobierane z serwera
    "TEST-KEY-12345",
    "DUCKY-2024-KEY"
}

-- Funkcja weryfikacji klucza
local function VerifyKey(key)
    -- Sprawd≈∫ czy klucz jest w li≈õcie wa≈ºnych kluczy
    for _, validKey in ipairs(ValidKeys) do
        if key == validKey then
            return true
        end
    end
    return false
end

