-- Funkcje przeciÄ…gania
local dragging = false
local dragStart = nil
local startPos = nil

TopBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - dragStart
        MainFrame.Position = UDim2.new(
            startPos.X.Scale,
            startPos.X.Offset + delta.X,
            startPos.Y.Scale,
            startPos.Y.Offset + delta.Y
        )
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)

-- Funkcja weryfikacji i Å‚adowania skryptu
local function LoadScript()
    print("Key verified! Loading script...")
end

-- ObsÅ‚uga przyciskÃ³w
CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

CheckKeyButton.MouseButton1Click:Connect(function()
    local enteredKey = KeyInputBox.Text
    if enteredKey == "" then
        KeyInputBox.Text = ""
        KeyInputBox.PlaceholderText = "Please enter a key!"
        TweenService:Create(KeyInputBox, TweenInfo.new(0.1), {BackgroundColor3 = Color3.fromRGB(220, 53, 69)}):Play()
        wait(0.1)
        TweenService:Create(KeyInputBox, TweenInfo.new(0.1), {BackgroundColor3 = Color3.fromRGB(28, 28, 38)}):Play()
        KeyInputBox.PlaceholderText = "Enter your key here..."
        return
    end
    
    if VerifyKey(enteredKey) then
        CheckKeyButton.Text = "Key Verified!"
        CheckKeyButton.BackgroundColor3 = Color3.fromRGB(34, 197, 94)
        LoadScript()
        wait(1)
        ScreenGui:Destroy()
    else
        CheckKeyButton.Text = "Invalid Key!"
        CheckKeyButton.BackgroundColor3 = Color3.fromRGB(220, 53, 69)
        KeyInputBox.Text = ""
        wait(1)
        CheckKeyButton.Text = "Check Key"
        CheckKeyButton.BackgroundColor3 = Color3.fromRGB(34, 197, 94)
    end
end)

GetKeyLinkButton.MouseButton1Click:Connect(function()
    local keyLink = "https://example.com/get-key"
    pcall(function()
        setclipboard(keyLink)
    end)
    GetKeyLinkButton.Text = "Link Copied!"
    wait(1)
    GetKeyLinkButton.Text = "Get Key Link"
end)

JoinDiscordButton.MouseButton1Click:Connect(function()
    local discordLink = "https://discord.gg/example"
    pcall(function()
        setclipboard(discordLink)
    end)
    JoinDiscordButton.Text = "Link Copied!"
    wait(1)
    JoinDiscordButton.Text = "Join Discord"
end)

-- Efekty hover
local function AddHoverEffect(button, normalColor, hoverColor)
    button.MouseEnter:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.15), {BackgroundColor3 = hoverColor}):Play()
    end)
    button.MouseLeave:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.15), {BackgroundColor3 = normalColor}):Play()
    end)
end

AddHoverEffect(CloseButton, Color3.fromRGB(220, 53, 69), Color3.fromRGB(239, 68, 68))
AddHoverEffect(CheckKeyButton, Color3.fromRGB(34, 197, 94), Color3.fromRGB(74, 222, 128))
AddHoverEffect(GetKeyLinkButton, Color3.fromRGB(99, 102, 241), Color3.fromRGB(129, 140, 248))
AddHoverEffect(JoinDiscordButton, Color3.fromRGB(88, 101, 242), Color3.fromRGB(99, 102, 241))
AddHoverEffect(SidebarKeyButton, Color3.fromRGB(59, 130, 246), Color3.fromRGB(96, 165, 250))

-- Aktualizacja rozmiaru ContentScrolling
ContentLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
    ContentScrolling.CanvasSize = UDim2.new(0, 0, 0, ContentLayout.AbsoluteContentSize.Y + 20)
end)

print("Verification GUI zaÅ‚adowane!")
